package week1.day2;

public class IsPrime {
	
public static void main(String[] args) {
	
int num=13;

for(int i=0;i<10;i++) {
if((num/1==num)&&(num/num==1)){
System.out.println("the given nos is prime"+num);
}
//else ifo(num/num!=0){
//System.out.println("the given nos is prime"+num);	
//}
else {
System.out.println("the given nos is not prime"+num);	
}
num++;
//System.out.println("the given nos is prime"+num);
}	
	
}	

}
